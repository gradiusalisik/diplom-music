.cooperwinds
	&__wrap
		background-image url('../images/bg-cooperwinds.jpg')
		background-size cover

	&__title
		margin-bottom 20px
		text-align center

	&__content
		margin-bottom 15px
		display flex
		justify-content space-between
		align-items center
		width 100%

	&__footer
		position absolute
		left 0
		bottom 35px
		display flex
